<Page
    x:Class="Practice.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Practice"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <TextBlock x:ConnectionId='2' x:Name="textBlock" 
            HorizontalAlignment="Left"
            TextWrapping="Wrap"
            Text="Contact List"
            FontSize="20"
            VerticalAlignment="Top"
            Margin="448,133,0,0" RenderTransformOrigin="0.489,1.527"/>
        <TextBlock x:ConnectionId='3' x:Name="textBlock1" 
                   HorizontalAlignment="Left"
                   TextWrapping="Wrap"
                   VerticalAlignment="Top"
                   Width="440"
                   Height="419" Margin="272,200,0,0"/>
        <Button x:ConnectionId='4' x:Name="Addct" 
            Content="Add Contact" 
            VerticalAlignment="Top"
            FontSize="20"
            Margin="273,650,0,0"
            Width="168" Height="39"
                               />
        <Button x:ConnectionId='5' x:Name="Searchct" 
            Content="Search Contact" 
            VerticalAlignment="Top"
            FontSize="20"
            Margin="533,650,0,0"
            Width="172" Height="39"
                                  />
    </Grid>
</Page>

